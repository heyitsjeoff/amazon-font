{
  "manifest_version": 2,
  "name": "amazon-font",
  "description": "overrides the amazon font with Amazon Ember",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "https://www.amazon.com/*",
        "https://amazon.com/*"
      ],
      "run_at": "document_start",
      "js": ["content.js"]
    }
  ]
}